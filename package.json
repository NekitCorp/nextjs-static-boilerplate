{
    "name": "nextjs-static-boilerplate",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/nekitcorp/nextjs-static-boilerplate.git"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "export": "next export",
